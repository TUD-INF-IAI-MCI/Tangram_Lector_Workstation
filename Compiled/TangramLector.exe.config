    <!--DrawingSFP SSFP configuration-->
    <add key="DrawingSFP_BBoxpercentage" value="0.5"/>
    <add key="DrawingSFP_MinBBoxSize" value="5"/>
    <!--DrawingDebugSFP configuration-->
    <add key="DrawingDebugSFP_StorePath" value="C:\test\Tempfiles"/>
    <add key="DrawingDebugSFP_TempFilePrepend" value="tmp_"/>
    <add key="DrawingDebugSFP_StoreTempFiles" value="True"/>
    <add key="DrawingDebugSFP_TempFileStoreInterval" value="20000"/>
    <add key="DrawingDebugSFP_LogTouchData" value="False"/>
    <add key="DrawingDebugSFP_Active" value="True"/>
    <add key="DrawingDebugSFP_AutoSave" value="True"/>
    <add key="DrawingSFP_LabelPadding" value="500 500 300"/>
    <add key="DrawingSFP_ConnectorType" value="LINES"/>
    <add key="DebugOutput" value="True"/>
    <add key="DebugOutputDir" value="C:\test\"/>
   
    




