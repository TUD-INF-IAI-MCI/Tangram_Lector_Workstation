    <add key="Audio_Volume" value="80" />
    <add key="Audio_Speed" value="5" />
    <!--Logger config-->
    <add key="-DefaultLogPriority" value="debug" />
    <!--ShowOff Adapter config-->
    <add key="ShowOff_ShowScreenshotMenu" value="true" />
