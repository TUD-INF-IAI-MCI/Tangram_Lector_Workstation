  <userSettings>
    <tud.mci.tangram.TangramLector.Properties.Settings>
      <setting name="sound_volume" serializeAs="String">
        <value>100</value>
      </setting>
      <setting name="sound_speed" serializeAs="String">
        <value>5</value>
      </setting>
      <setting name="sound_voice" serializeAs="String">
        <value />
